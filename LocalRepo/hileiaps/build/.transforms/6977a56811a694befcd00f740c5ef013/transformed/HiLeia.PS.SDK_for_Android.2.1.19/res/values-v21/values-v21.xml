<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="no_video">没有视频记录！</string>
    <style name="AppTheme" parent="BaseAppThemeLight">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:windowContentTransitions">true</item>
    </style>
    <style name="EasyPhotosFullscreenTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="metaButtonBarStyle">?android:attr/buttonBarStyle</item>
        <item name="metaButtonBarButtonStyle">?android:attr/buttonBarButtonStyle</item>
        <item name="android:statusBarColor">@color/easy_photos_status_bar</item>
    </style>
    <style name="EasyPhotosTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">@color/easy_photos_status_bar</item>
        <item name="android:windowIsTranslucent">false</item>
    </style>
</resources>